{
  "name": "Skip Netflix Plugin",
  "version": "1.0",
  "manifest_version": 3,
  "description": "Skip Netflix Plugin",
  "permissions": [
    "activeTab",
    "scripting",
    "storage",
    "tabs"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "https://www.netflix.com/watch/*"
  ]
}